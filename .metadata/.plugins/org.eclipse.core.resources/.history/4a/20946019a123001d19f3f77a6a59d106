	<parameter name="DATE_FROM" class="java.lang.String"/>
	<parameter name="DATE_TO" class="java.lang.String"/>
	<parameter name="SERVICE_IDS" class="java.lang.String"/>
	<parameter name="ONLY_MEASURE" class="java.lang.String"/>
	<parameter name="REPORT_NAME" class="java.lang.String"/>
	<parameter name="REPORT_CODE" class="java.lang.String"/>
	<parameter name="REPORT_PARAMETERS_LIST" class="java.lang.String"/>
	<queryString language="plsql">
		<![CDATA[SELECT sd.text               AS service_name
     , w.text                AS work_name
     , count(DISTINCT oi.id) AS cnt
  FROM lab.research r
  JOIN lab.order_info oi ON oi.research_id = r.id
  JOIN lab.orderworkflow owf ON owf.order_id = oi.id
  JOIN lab.workflow wf ON wf.id = owf.workflow_id
  JOIN lab.work w ON w.id = wf.work_id
  JOIN med.srvdep sd ON sd.keyid = oi.srvdep_id
  JOIN LATERAL (SELECT CASE
                         WHEN lcode IN ('1') THEN 1
                         WHEN lcode IN ('2','3','4','5') THEN 5
                         WHEN code IN (1) THEN 2
                         WHEN code IN (3) THEN 3
                         WHEN code IN (2,5,6) THEN 4
                         ELSE 6
                       END AS work_sort
                  FROM lab.lu
                 WHERE id = coalesce(w.action_id, w.typ_id)
                 LIMIT 1) wt ON TRUE
 WHERE r.status IN ('A','R','F')
   AND oi.ok_status = 1
   AND oi.check_status = 1
   AND owf.ok_status = 1
   AND (nullif($P{ONLY_MEASURE}, '0') IS NULL OR ($P{ONLY_MEASURE} = '1' AND work_sort = 4))
   AND ($P{SERVICE_IDS} IS NULL OR oi.srvdep_id = ANY (string_to_array($P{SERVICE_IDS}, ', ')::BIGINT[]))
   AND date_trunc('day', oi.finish_date) BETWEEN to_date($P{DATE_FROM}, 'yyyy-mm-dd hh24:mi:ss') AND coalesce(
     to_date($P{DATE_TO}, 'yyyy-mm-dd hh24:mi:ss'), current_date)
 GROUP BY GROUPING SETS ((sd.text), (sd.text,w.text,work_sort))
 ORDER BY service_name, work_sort NULLS FIRST, work_name NULLS FIRST]]>
	</queryString>
	<field name="service_name" class="java.lang.String"/>

	<parameter name="RESEARCH_ID" class="java.lang.String"/>
	<queryString language="plsql">
		<![CDATA[select (select l.text from lab.lu l where l.id = r.category_id) AS category
     , a.finance AS fincode
     ,(select l.text from med.lu l where l.tag = 5 and l.code = a.finance) AS finance
     ,(select c.text from med.company c where c.keyid = a.companyid) AS company
     ,(select string_agg(c.text, ', ' order by c.text)
       from lab.research_conditions rc
          ,lab.condition           c
       where rc.condition_id = c.id
         and rc.research_id = r.id) AS pat_condition
     ,med.pkg_lab_utils__get_r_cust_text(r.id) as direct
     ,med.pkg_lab_utils__get_r_dirdoc_text(r.id) as direct_doctor
     ,r.research_num AS direct_num
     ,to_char(r.income_date, 'dd.MM.yyyy') AS direct_date
     ,r.orderid
     ,to_char(r.regdate, 'dd.MM.yyyy') AS reg_date
     ,(select l.text from lab.lu l where l.id = r.research_goal_id) AS research_goal
     ,(select string_agg(d.code, ', ' order by d.code)
       from lab.research_diagnos rd
          ,med.diagnos          d
       where rd.diagnos_id = d.keyid
         and rd.research_id = r.id) as diagnos
,(select STRING_AGG((d.code || ' ' ||  substring(d.text from 1 for 30)), '; ' ORDER BY d.code)
          from lab.research_diagnos rd
              ,med.diagnos          d
         where rd.diagnos_id = d.keyid
           and rd.research_id = r.id) as diagnos_text
     ,r.note
     ,(select string_agg(concat(fi.text, ': ', frv.text),chr(10) order by fi.sortcode)
       from form.form f
          , form.form_item fi
          , reg.form_result_labdirect fr
          , reg.form_result_value_labdirect frv
       where f.id = fi.form_id
         and fr.id = frv.form_result_id
         and fi.id = frv.form_item_id
         and fr.link_id = r.id
         and f.code = 'LABDIRECT_ADDITIONAL_FORM') as add_data
from lab.research r
         left join
     med.agr a ON a.keyid = r.agr_id
where r.id = cast ($P{RESEARCH_ID} as bigint)]]>
	</queryString>

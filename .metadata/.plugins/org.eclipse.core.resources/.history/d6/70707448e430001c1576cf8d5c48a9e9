	<parameter name="RESEARCH_ID" class="java.lang.String"/>
	<queryString language="plsql">
		<![CDATA[select (select l.text from solution_lab.lu l where l.id = r.category_id) AS category
      , a.finance AS fincode
      --,(select l.text from solution_med.lu l where l.tag = 5 and l.code = a.finance) AS finance
      ,(select FN_GET_TRANSLIT(l.text) from solution_med.lu l where l.tag = 5 and l.code = a.finance) AS finance
      --,(select c.text from company c where c.keyid = a.companyid) AS company
      ,(select FN_GET_TRANSLIT(c.text) from company c where c.keyid = a.companyid) AS company
      ,(select listagg(nvl(c.note,c.text), ', ') within GROUP(ORDER BY c.text)
          from solution_lab.research_conditions rc
              ,solution_lab.condition           c
         where rc.condition_id = c.id
           and rc.research_id = r.id) AS pat_condition
      --,p_lab_utils.get_r_cust_text(r.id) as direct
      ,(case WHEN r.external_customer_id is not null
              THEN (select nvl(text_eng,FN_GET_TRANSLIT(text)) from solution_lab.lu where id = r.external_customer_id)
             WHEN r.internal_customer_id is not null
              THEN (select FN_GET_TRANSLIT(text) from solution_med.dep where keyid = r.internal_customer_id)
             ELSE null END) as direct
      --,p_lab_utils.get_r_dirdoc_text(r.id) as direct_doctor
      ,trim(FN_GET_TRANSLIT(nvl(p_lab_utils.get_r_dirdoc_text(r.id),' '), 9303)) as direct_doctor
      ,r.research_num AS direct_num
      ,to_char(r.income_date, 'dd.MM.yyyy') AS direct_date
      ,r.orderid
      ,to_char(r.regdate, 'dd.MM.yyyy') AS reg_date
      --,(select l.text from solution_lab.lu l where l.id = r.research_goal_id) AS research_goal
      ,(select nvl(l.text_eng,FN_GET_TRANSLIT(l.text)) from solution_lab.lu l where l.id = r.research_goal_id) AS research_goal
      ,(select listagg(d.code, ', ') within GROUP(ORDER BY d.code)
          from solution_lab.research_diagnos rd
              ,solution_med.diagnos          d
         where rd.diagnos_id = d.keyid
           and rd.research_id = r.id) as diagnos
       --,r.note
       ,trim(FN_GET_TRANSLIT(nvl(r.note,' '))) as note
       ,(select listagg(fi.text||': '||decode(fi.data_type,2,substr(frv.text,12),frv.text),chr(10)) within group (order by fi.sortcode)
           from solution_form.form f
              , solution_form.form_item fi
              , solution_reg.form_result_labdirect fr
              , solution_reg.form_result_value_labdirect frv
          where f.id = fi.form_id
            and fr.id = frv.form_result_id
            and fi.id = frv.form_item_id
            and fr.link_id = r.id
            and f.code = 'LABDIRECT_ADDITIONAL_FORM') as add_data
  from solution_lab.research r
  left join 
       agr a ON a.keyid = r.agr_id
where r.id = $P{RESEARCH_ID}]]>
	</queryString>

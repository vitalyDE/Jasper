	<queryString language="plsql">
		<![CDATA[with tab as (
select rownum rn
         , column_value 
 from table(P_COLLECTIONS.string_to_table_of_id($P{RESEARCH_ID}))
)
select distinct
           r.id as research_id
         , p_lab.get_research_result_type(r.id) as research_type
         , p_lab.get_result_for_report_count(r.id, 1) as result_count
  from solution_lab.research r
          , tab t
 where (r.root_id = column_value OR (r.id = column_value AND r.root_id is not null)) 
order by (select rn from tab where r.id = column_value)]]>
	</queryString>

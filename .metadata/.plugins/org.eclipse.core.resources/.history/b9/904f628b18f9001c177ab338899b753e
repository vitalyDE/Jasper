		<![CDATA[SELECT --l.text AS place,
             l.note AS tel
 ,fn_translate_text(l.text, $P{LANG}) as place
 ,fn_translate_text('Министерство здравоохранения Свердловской области', $P{LANG}) as lpu1
 ,fn_translate_text('ГОСУДАРСТВЕННОЕ АВТОНОМНОЕ УЧРЕЖДЕНИЕ ЗДРАВООХРАНЕНИЯ СВЕРДЛОВСКОЙ ОБЛАСТИ', $P{LANG}) as lpu2
 ,fn_translate_text('"КЛИНИКО-ДИАГНОСТИЧЕСКИЙ ЦЕНТР ГОРОД ЕКАТЕРИНБУРГ"', $P{LANG}) as lpu3
 ,fn_translate_text('620144, г. Екатеринбург, ул. 8 Марта 78-В, ОГРН 1026605247415', $P{LANG}) as recv_1
 ,fn_translate_text('E-mail: inbox@kdc-lab.ru, ИНН: 6661028380, КПП: 667101001', $P{LANG}) as recv_2
 ,fn_translate_text('Администрация: (343) 204-71-34, факс: (343) 204-71-57, доб. № 559', $P{LANG}) as recv_3
 ,fn_translate_text('Регистратура: (343) 204-71-64, (343) 204-71-54', $P{LANG}) as recv_4
 ,fn_translate_text('тел', $P{LANG}) as f_tel
   FROM solution_lab.order_info oi
      , solution_lab.orderworkflow owf
      , solution_lab.workflow wf
      --, solution_lab.wrkworkplace wwp
      , solution_lab.work w
      , solution_lab.workplace wp
      , solution_lab.lu l
  WHERE oi.research_id = $P{RESEARCH_ID}
    AND oi.id = owf.order_id
    AND wf.id = owf.workflow_id
    AND wf.work_id = w.id
    AND w.workplace_id = wp.id
    AND l.id = wp.place_id
    --AND owf.ok_status = 1
    AND owf.check_status = 1
    AND ROWNUM = 1]]>
	</queryString>

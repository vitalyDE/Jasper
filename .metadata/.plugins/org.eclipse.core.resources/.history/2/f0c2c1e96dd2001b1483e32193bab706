	<parameter name="RESEARCH_ID" class="java.lang.String"/>
	<queryString language="plsql">
		<![CDATA[select (SELECT coalesce(w.shorttext,wf.text)
              FROM solution_lab.orderworkflow owf
                         ,solution_lab.workflow      wf
                         ,solution_lab.work w
                 WHERE owf.id = mr.orderworkflow_id
                 AND owf.workflow_id = wf.id
                 and w.id = wf.work_id) AS WORK
     , mb.sortcode as bactery_sortcode
     , mb.bactery_text
     , mb.note as bactery_note
     , decode(e,0,mb.koe,'') koe
     , decode(e,0,'',replace(substr(koe,0,e-1),',0','') || '*') koe1
--     , decode(e,0,'',substr(koe,0,e-1) || '*10') koe1
     , decode(e,0,'',to_char(to_number(substr(koe,e+2,2)))) koe2
     , decode(e,0,'',mb.koe_units) koe_units
     , ma.sortcode as ab_sortcode
     , ma.antibiotic_text
     , decode(ma.sir, 1, 'S', 2, 'I', 3, 'R') AS sir
     , ma.dia
     , ma.mic
     , mr.note
     , mr.conclusion
      ,fn_man_short_by_id(mr.executor_id) as executor
      ,fn_man_short_by_id(mr.verifier_id)  as verifier
  from solution_lab.order_info oi
     , solution_lab.mbio_result mr
     , (select t.*,NVL(INSTR(KOE,'E+'),0) e 
          from solution_lab.mbio_bactery t) mb
     , solution_lab.mbio_antibiotic ma
 where oi.research_id = $P{RESEARCH_ID}
   and mr.order_info_id = oi.id
   and mr.id = mb.mbio_result_id (+)
   and mb.id = ma.mbio_bactery_id (+)
 order by bactery_sortcode, ab_sortcode]]>
	</queryString>

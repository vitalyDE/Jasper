	<parameter name="PATIENT_ID" class="java.lang.String"/>
	<queryString language="plsql">
		<![CDATA[select initcap(concat_ws(' ', p.lastname, p.firstname, p.secondname)) AS fio
     ,p.num AS patnum
     ,to_char(p.birthdate, 'dd.MM.yyyy') AS bdate
     ,med.fn_pat_age_by_id(p.keyid) AS age
     ,(case p.sex when 0 then 'Муж' when 1 then 'Жен' end) as sex
     ,coalesce(p.cellular,p.phone) AS phone
	 ,p.snils as snils
	 ,p.passser || ' ' || p.passnumb as pass_data
     ,med.gsp_get_pat_address(p.keyid, 1::smallint, 0::smallint) AS address
     ,med.gsp_get_pat_address(p.keyid, 2::smallint, 0::smallint) AS address1
from med.patient p
where p.keyid = cast ($P{PATIENT_ID} as bigint)]]>
	</queryString>

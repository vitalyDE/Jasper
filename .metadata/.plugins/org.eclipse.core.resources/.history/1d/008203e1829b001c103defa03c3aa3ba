	<queryString language="plsql">
		<![CDATA[WITH tab AS
 (SELECT rownum rn
               ,column_value
    FROM TABLE(p_collections.string_to_table_of_id($P{DOCUMENTS})))
select doc.filename
           ,doc.content
           ,null as pageImage
  FROM solution_data.documents doc
             , tab t
 where doc.id = t.column_value
      and doc.tag = 9
      and doc.status = 1
      and lower(filename) like '%.pdf'
order by rn]]>
	</queryString>

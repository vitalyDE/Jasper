	<parameter name="RESEARCH_ID" class="java.lang.String"/>
	<parameter name="ROOT_REPORT_PATH" class="java.lang.String"/>
	<queryString language="plsql">
		<![CDATA[select r.ids
           ,to_char(r.regdate,'dd.mm.yyyy') as reg_date
           ,fn_pat_name_by_id(r.patient_id) fio
           ,coalesce(to_char(p.birthdate, 'dd.MM.yyyy'),'                           ') AS bdate
           ,coalesce(p.cellular,p.phone,'                           ') AS phone
           ,coalesce(p.note,'                           ') as email
           ,coalesce(gsp_get_pat_address(p.keyid, 1, 0),'                           ') AS address
           ,coalesce(gsp_get_pat_address(p.keyid, 2, 0),'                           ') AS address1
           ,coalesce(fn_pat_pass_str(r.patient_id),'                           ') AS pass
           ,(select upper(trim(text)) from solution_lab.lu where id = r.external_customer_id) as customer
  from solution_lab.research r
          , solution_med.patient p
where r.id = $P{RESEARCH_ID}
     and r.patient_id = p.keyid]]>
	</queryString>

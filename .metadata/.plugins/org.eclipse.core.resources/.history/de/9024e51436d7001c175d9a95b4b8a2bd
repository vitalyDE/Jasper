	<parameter name="PATIENT_ID" class="java.lang.String"/>
	<queryString language="plsql">
		<![CDATA[select initcap(p.lastname || ' ' || p.firstname || ' ' || p.secondname) AS fio
             ,p.num AS patnum
             ,to_char(p.birthdate, 'dd.MM.yyyy') AS bdate
             ,fn_pat_age_by_id(p.keyid) AS age
             ,decode(p.sex, 0, 'Муж', 1, 'Жен') as sex
             ,coalesce(p.cellular,p.phone) AS phone
             ,gsp_get_pat_address(p.keyid, 1, 0) AS address
             ,gsp_get_pat_address(p.keyid, 2, 0) AS address1
			,p.snils as snils
			,p.passser || ' ' || p.passnumb as pass_data
  from patient p
 where p.keyid = $P{PATIENT_ID}]]>
	</queryString>

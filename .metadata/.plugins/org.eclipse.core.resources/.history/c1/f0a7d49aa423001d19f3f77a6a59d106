	<parameter name="DATE_FROM" class="java.lang.String"/>
	<parameter name="DATE_TO" class="java.lang.String"/>
	<parameter name="SPLIT_WORKPLACES" class="java.lang.String"/>
	<parameter name="DEPARTMENT_IDS" class="java.lang.String"/>
	<parameter name="WORKPLACE_IDS" class="java.lang.String"/>
	<parameter name="REPORT_NAME" class="java.lang.String"/>
	<parameter name="REPORT_CODE" class="java.lang.String"/>
	<parameter name="REPORT_PARAMETERS_LIST" class="java.lang.String"/>
	<queryString language="plsql">
		<![CDATA[SELECT CASE
         WHEN $P{SPLIT_WORKPLACES} = '1' THEN wp.text
         ELSE NULL
       END      AS workplace
     , w.text   AS work_name
     , count(1) AS cnt
  FROM lab.research r
  JOIN lab.order_info oi ON oi.research_id = r.id
  JOIN lab.orderworkflow owf ON owf.order_id = oi.id
  JOIN lab.workflow wf ON wf.id = owf.workflow_id
  JOIN lab.work w ON w.id = wf.work_id
  JOIN lab.workplace wp ON wp.id = w.workplace_id
 WHERE r.root_id IS NOT NULL
   AND r.status IN ('A','R','F')
   AND owf.ok_status = 1
   AND owf.check_status = 1
   AND date_trunc('day', owf.finishdate) BETWEEN to_date($P{DATE_FROM}, 'yyyy-mm-dd hh24:mi:ss') AND coalesce(
     to_date($P{DATE_TO}, 'yyyy-mm-dd hh24:mi:ss'), current_date)
   AND ($P{DEPARTMENT_IDS} IS NULL OR
        wp.place_id = ANY (string_to_array($P{DEPARTMENT_IDS}, ',')::BIGINT[]))
   AND ($P{WORKPLACE_IDS} IS NULL OR
        wp.id = ANY (string_to_array($P{WORKPLACE_IDS}, ',')::BIGINT[]))
 GROUP BY 1, 2
 ORDER BY 1, 2]]>
	</queryString>

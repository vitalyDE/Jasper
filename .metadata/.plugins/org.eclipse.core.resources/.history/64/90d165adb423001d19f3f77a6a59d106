	<queryString language="plsql">
		<![CDATA[SELECT coalesce(l.text, 'Не определен') AS customer
     , count(DISTINCT r.patient_id)     AS cnt_p
     , count(r.id)                      AS cnt_r
  FROM lab.research r
  LEFT JOIN lab.lu l ON l.id = r.external_customer_id
 WHERE r.root_id IS NOT NULL
   AND coalesce(r.cancel_status, 0) = 0
   AND date_trunc('day', r.regdate) BETWEEN to_date($P{DATE_FROM},'yyyy-mm-dd hh24:mi:ss') AND coalesce(to_date($P{DATE_TO},'yyyy-mm-dd hh24:mi:ss'), current_date)
 GROUP BY l.text
 ORDER BY l.text]]>
	</queryString>
	<field name="CUSTOMER" class="java.lang.String"/>

	<parameter name="DATE_FROM" class="java.lang.String"/>
	<parameter name="DATE_TO" class="java.lang.String"/>
	<parameter name="EXECUTORS" class="java.lang.String"/>
	<parameter name="REPORT_NAME" class="java.lang.String"/>
	<parameter name="REPORT_CODE" class="java.lang.String"/>
	<parameter name="REPORT_PARAMETERS_LIST" class="java.lang.String"/>
	<queryString language="plsql">
		<![CDATA[SELECT coalesce(res.executor, owf.executor) as executor
     , w.code                        AS work_sort
     , coalesce(w.shorttext, w.text) AS work_name
     , count(1)                      AS cnt
  FROM lab.research r
  JOIN lab.order_info oi ON oi.research_id = r.id
  JOIN lab.orderworkflow owf ON owf.order_id = oi.id
  JOIN lab.workflow wf ON wf.id = owf.workflow_id
  JOIN lab.work w ON w.id = wf.work_id
  LEFT JOIN LATERAL (SELECT res.executor_id
                          , res.executor
                       FROM lab.result res
                      WHERE res.orderworkflow_id = owf.id
                        AND res.check_status = 1
                        AND res.active_status = 1
                      ORDER BY res.resultdate DESC
                      LIMIT 1) res on true
 WHERE r.status IN ('A','R','F')
   AND owf.result_status IN (1,2)
   AND owf.check_status IN (1,2)
   AND owf.status = 1
   AND w.typ_id IN (SELECT id
                      FROM lab.lu
                     WHERE tag = 16
                       AND code IN (2,5))
   AND ($P{EXECUTORS} IS NULL OR
        res.executor_id = ANY (string_to_array($P{EXECUTORS}, ', ')::bigint[]))
   AND date_trunc('day', r.collectdate) BETWEEN to_date($P{DATE_FROM}, 'yyyy-mm-dd hh24:mi:ss') AND coalesce(
     to_date($P{DATE_TO}, 'yyyy-mm-dd hh24:mi:ss'), current_date)
 GROUP BY 1, 2, 3
 ORDER BY 1, 2, 3]]>
	</queryString>

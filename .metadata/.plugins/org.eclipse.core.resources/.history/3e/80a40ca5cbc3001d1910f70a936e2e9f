	<parameter name="RESEARCH_ID" class="java.lang.String"/>
	<parameter name="LANG" class="java.lang.String"/>
	<queryString language="plsql">
		<![CDATA[SELECT med.fn_translate_text('Заказ №', $P{LANG})        AS orderid_title
     , med.fn_translate_text('№ направления', $P{LANG})  AS direct_num_title
     , med.fn_translate_text(coalesce(nullif(trim(med.pkg_admin__fn_get_param_value_text('599012')), '')
                               , '№ ИБ'), $P{LANG})      AS med_history_num_title
     , med.fn_translate_text('Заказчик', $P{LANG})       AS direct_title
     , med.fn_translate_text('Направление от', $P{LANG}) AS direct_date_title
     , med.fn_translate_text('Врач', $P{LANG})           AS direct_doctor_title
     , med.fn_translate_text('Категория', $P{LANG})      AS category_title
     , med.fn_translate_text('Состояние', $P{LANG})      AS condition_title
     , med.fn_translate_text('Диагноз', $P{LANG})        AS diagnos_title
     , med.fn_translate_text('Цель исслед.', $P{LANG})   AS research_goal_title
     , med.fn_translate_text('Комментарий', $P{LANG})    AS note_title
     , r.orderid
     , r.research_num
     , r.med_history_num
     , to_char(r.regdate, 'dd.MM.yyyy')                  AS reg_date
     , to_char(r.income_date, 'dd.MM.yyyy')              AS direct_date
     , a.finance                                         AS fincode
     , med.fn_translate_text(fin.text, $P{LANG})         AS finance
     , med.fn_translate_text(comp.text, $P{LANG})        AS company
     , med.fn_translate_text(cat.text, $P{LANG})         AS category
     , med.fn_translate_text(cust.text, $P{LANG})        AS direct
     , med.fn_translate_text(doc.text, $P{LANG})         AS direct_doctor
     , med.fn_translate_text(purp.text, $P{LANG})        AS research_goal
     , med.fn_translate_text(r.note, $P{LANG})           AS note
     , (SELECT string_agg(med.fn_translate_text(c.text, $P{LANG}), ', ' ORDER BY c.text)
          FROM lab.research_conditions rc
             , lab.condition c
         WHERE rc.condition_id = c.id
           AND rc.research_id = r.id)                    AS pat_condition
     , (SELECT string_agg(d.code, ', ' ORDER BY d.code)
          FROM lab.research_diagnos rd
             , med.diagnos d
         WHERE rd.diagnos_id = d.keyid
           AND rd.research_id = r.id)                    AS diagnos
     , (SELECT string_agg(med.fn_translate_text(fi.text, $P{LANG}) || ': ' || med.fn_translate_text(frv.text, $P{LANG}),
                          chr(10) ORDER BY fi.sortcode)
          FROM form.form f
             , form.form_item fi
             , reg.form_result_labdirect fr
             , reg.form_result_value_labdirect frv
         WHERE f.id = fi.form_id
           AND fr.id = frv.form_result_id
           AND fi.id = frv.form_item_id
           AND fr.link_id = r.id
           AND f.code = 'LABDIRECT_ADDITIONAL_FORM')     AS add_data
  FROM lab.research r
  LEFT JOIN med.agr a ON a.keyid = r.agr_id
  LEFT JOIN med.company comp ON comp.keyid = a.companyid
  LEFT JOIN med.lu fin ON fin.tag = 5 AND fin.code = a.finance
  LEFT JOIN lab.lu cust ON cust.id = r.external_customer_id
  LEFT JOIN lab.lu doc ON doc.id = r.direct_doctor_id
  LEFT JOIN lab.lu cat ON cat.id = r.category_id
  LEFT JOIN lab.lu purp ON purp.id = r.research_goal_id
 WHERE r.id = $P{RESEARCH_ID}::BIGINT]]>
	</queryString>
